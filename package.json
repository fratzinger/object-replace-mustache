{
  "name": "object-replace-mustache",
  "version": "3.3.0-0",
  "description": "replace placeholders of an object with a view like you would use mustache.render for strings",
  "author": "fratzinger",
  "homepage": "https://github.com/fratzinger/object-replace-mustache",
  "repository": {
    "type": "git",
    "url": "https://github.com/fratzinger/object-replace-mustache"
  },
  "keywords": [
    "object",
    "replace",
    "mustache",
    "placeholder"
  ],
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "src/**",
    "dist/**"
  ],
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "build": "tsdown",
    "version": "pnpm run build",
    "release": "np",
    "test": "vitest run",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint \"{src,test}/**/*{.js,.ts}\" --ext .js,.ts"
  },
  "dependencies": {
    "meriyah": "^6.1.4"
  },
  "devDependencies": {
    "@feathers-community/eslint-config": "^0.0.9",
    "@types/node": "^24.10.0",
    "@vitest/coverage-v8": "^4.0.7",
    "eslint": "^9.39.1",
    "np": "^10.2.0",
    "pkg-pr-new": "^0.0.60",
    "shx": "^0.4.0",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  }
}
